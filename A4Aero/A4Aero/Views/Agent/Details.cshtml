@model A4Aero.Models.AgentList
@{
    ViewBag.Title = "Model.Name";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Agent Info</h2>

<table class="table table-bordered table-hover">
    <tr>
        <td>
            <b>ID/Code:</b>
        </td>
        <td>@Model.CODE</td>
    </tr>
    <tr>
        <td>
            <b>Name:</b>
        </td>
        <td>@Model.NAME</td>
    </tr>
    <tr>
        <td>
            <b>Markup Plan:</b>
        </td>
        <td>@Model.MARKUP_PLAN</td>
    </tr>
    <tr>
        <td>
            <b>Mobile:</b>
        </td>
        <td>@Model.MOBILE</td>
    </tr>
    <tr>
        <td>
            <b>Email:</b>
        </td>
        <td>@Model.EMAIL</td>
    </tr>
</table>
@Html.ActionLink("Back to Agent List page", "Index")

